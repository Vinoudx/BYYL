const n=10;
var i,j;
/*if and do-while*/
begin

    i := 1;
    j := 1;
    if (i >= n)
        begin
            j:=j+1;
            write(i)
        end;
    else 
        begin
            j:=i+2;
            write(j)
        end;
    ;

    j:=1;
    do
	    begin
	        j:=j+1;
	        write(j)
	    end;
	while(j<=n);

end.