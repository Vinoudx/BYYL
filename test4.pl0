const n = 10;
var i;
/*switch-case*/
begin
	i := 2;
	
	switch i:
	case 1:
		begin
			i:=i+5;
			write(i);
		end;
		break;
	case 2:
		begin
			i:=i+6;
			write(i);	
		end;
	case 3:
		begin
			i:=i+7;
			write(i);	
		end;
		break;
	default:
		begin
			i:=i+8;
			write(i);	
		end;
		break;
	endswitch

end.